getcnname=function(str)
	if #str%2~=0 then return str end
	local xinglength=4
	local xingstr=""
	xingstr=cnxing[string.sub(str,1,xinglength)]
	if xingstr==nil then
		xinglength=2
		xingstr=cnxing[string.sub(str,1,xinglength)]
		if xingstr==nil then
			return nil
		end
	end
	local minglength=#str-xinglength
	local mingstr=""
	local tname=""
	for i=1,minglength,2 do
		tname=cnname[string.sub(str,i+xinglength,i+xinglength+1)]
		if tname==nil then
			return nil
		else
			mingstr=mingstr..tname
		end
	end
	return xingstr.." "..mingstr
end
